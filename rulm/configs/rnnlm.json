{
   "model": {
        "dropout": 0.3,
        "embedder": {
            "type": "random",
            "embedding_dim": 250
        },
        "contextualizer": {
            "type": "lstm",
            "input_dim": 250,
            "output_dim": 250,
            "n_layers": 1,
            "lstm_hidden_size": 250,
            "lstm_dropout": 0.3,
            "projection_dropout": 0.3
        }
    },
    "train": {
        "dataset": {
            "type": "stream"
        },
        "batch_size": 64,
        "epochs": 20,
        "optimizer": {
            "type": "adam",
            "lr": 0.001
        },
        "validate_every": 1,
        "serialization_dir": "current_model"
    }
}
